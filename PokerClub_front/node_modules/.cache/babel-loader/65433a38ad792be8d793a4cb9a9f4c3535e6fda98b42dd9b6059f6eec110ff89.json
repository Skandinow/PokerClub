div>\n  </div>\n</template>\n\n<style scoped>\n//.enter {\n//  position: absolute;\n//  top: 50%;\n//  left: 40%;\n//  color: #bec7c7;\n//}\n\n\n:root {\n  --border-color: rgb(185, 185, 185);\n  --border-radius: 6px;\n  --black-color: #333;\n  --caption-color: #3B5998;\n  --error-color: red;\n  --datatable-color: #E1E1E1;\n  --datatable-dark-cell-color: #f8f8f8;\n  --message-border-color: #1E88E5;\n  --message-background-color: #E3F2FD;\n  color: var(--black-color);\n  font-family: Verdana, sans-serif;\n  font-size: 16px;\n}\n\n.form-box {\n  border: 1px solid var(--border-color);\n  border-top-right-radius: var(--border-radius);\n  border-top-left-radius: var(--border-radius);\n  width: 30rem;\n  margin: 0 auto;\n}\n\n.form-box .header {\n  border-bottom: 1px solid var(--border-color);\n  color: var(--caption-color);\n  font-weight: bold;\n  padding: 0.25rem 0.5rem;\n}\n\n.form-box .field {\n  margin: 1.5rem 0;\n  overflow: hidden;\n}\n\n.form-box .field .name {\n  width: 11rem;\n  float: left;\n  text-align: right;\n  padding-right: 1rem;\n}\n\n.form-box .field .value {\n  width: 15rem;\n  float: left;\n}\n\n.form-box .field input {\n  width: 100%;\n}\n\n.form-box .button-field {\n  margin: 1.5rem 1.5rem 1.5rem 12rem;\n}\n\n.form-box .button-field input {\n  padding: 0.1rem 1.5rem;\n}\n\n.form-box .error {\n  /*clear: both;*/\n  color: var(--error-color);\n  font-size: 0.75rem;\n  margin-left: 9rem;\n}\n\n/* form */\n.form {\n  width: 36rem;\n  margin: 0 auto;\n}\n\n.form .header {\n  color: var(--caption-color);\n  font-weight: bold;\n  font-size: 1.4rem;\n  margin-bottom: 1rem;\n}\n\n.form .field {\n  margin-bottom: 1rem;\n}\n\n.form .field .name {\n}\n\n.form .field .value {\n  width: 35rem;\n}\n\n.form .field input, .form .field textarea {\n  width: 100%;\n}\n\n.form .field textarea {\n  min-height: 10rem;\n}\n\n.form .button-field {\n}\n\n.form .button-field input {\n  padding: 0.1rem 1.5rem;\n}\n\n.form .error {\n  color: var(--error-color);\n  font-size: 0.